{
  "name": "@podote-v2/server",
  "version": "2.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen:gql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.2.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/devtools-integration": "^0.1.3",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.11.1",
    "aws-sdk": "^2.1326.0",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.7",
    "firebase": "^9.17.1",
    "firebase-admin": "^11.5.0",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.18",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "^16.0.2",
    "nanoid": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.3",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.1",
    "@graphql-codegen/introspection": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.1",
    "@graphql-codegen/typescript-resolvers": "^3.1.0",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/firebase": "^3.2.1",
    "@types/graphql-upload": "^15.0.2",
    "@types/jest": "29.4.0",
    "@types/node": "^18.14.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.4.3",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "prisma-query-log": "^3.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
